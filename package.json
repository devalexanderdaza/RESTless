{
  "name": "restless",
  "description": "RESTless is a complete REST API that works 100% in the browser. Simulated REST API with data persistence that works entirely in the browser. A replacement for json-server without Node.js dependencies or external servers.",
  "version": "0.0.0",
  "private": false,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Alexander Daza",
    "email": "contacto@alexanderdaza.dev",
    "url": "https://www.alexanderdaza.dev"
  },
  "contributors": [
    {
      "name": "Alexander Daza",
      "email": "dev.alexander.daza@gmail.com",
      "url": "https://github.com/devalexanderdaza"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/devalexanderdaza/RESTless.git"
  },
  "bugs": {
    "url": "https://github.com/devalexanderdaza/RESTless/issues"
  },
  "homepage": "https://github.com/devalexanderdaza/RESTless#readme",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.2.0",
  "keywords": [
    "rest",
    "api",
    "json",
    "server",
    "rest-api",
    "restful",
    "rest-api-server",
    "rest-api-client",
    "rest-api-mock",
    "rest-api-mock-server",
    "rest-api-mock-client",
    "rest-api-mock-data",
    "serverless",
    "browser",
